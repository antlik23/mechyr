{
  "type": "module",
  "name": "sveltekit-starter",
  "scripts": {
    "dev:svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev:vite": "vite dev",
    "dev": "pnpm run --stream \"/^dev:.*/\"",
    "build:paraglide": "paraglide-js compile --project ./project.inlang",
    "build:vite": "vite build",
    "build": "pnpm run --sequential \"/^build:.*/\"",
    "test:unit": "vitest",
    "test:e2e": "playwright test --trace on",
    "test:e2e:update": "playwright test --update-snapshots",
    "test:ui": "playwright test --ui",
    "test:components": "playwright test -c playwright-ct.config.ts",
    "test:components:update": "playwright test -c playwright-ct.config.ts --update-snapshots",
    "preview": "vite preview",
    "check:svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint .",
    "check": "pnpm run \"/^check:.*/\"",
    "fix:prettier": "prettier --write .",
    "fix:eslint": "eslint --fix .",
    "fix": "pnpm run \"/^fix:.*/\"",
    "postinstall": "pnpm build:paraglide",
    "generate:types": "openapi-typescript https://beta-be.uzis.rascasone.com/api-docs/v1/swagger.json -o ./src/lib/api/types.ts --t && prettier ./src/lib/api/types.ts --write",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "translate": "npx @inlang/cli machine translate --project ./project.inlang -f"
  },
  "devDependencies": {
    "@eslint/js": "9.18.0",
    "@histoire/plugin-svelte": "0.17.17",
    "@inlang/paraglide-js": "1.11.7",
    "@inlang/paraglide-js-adapter-sveltekit": "0.6.7",
    "@inlang/paraglide-sveltekit": "^0.15.5",
    "@playwright/experimental-ct-svelte": "1.50.0",
    "@playwright/test": "1.50.0",
    "@poppanator/sveltekit-svg": "4.2.1",
    "@sveltejs/adapter-cloudflare": "4.8.0",
    "@sveltejs/kit": "2.9.1",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@tanstack/eslint-plugin-query": "5.62.1",
    "@tanstack/svelte-query-devtools": "5.62.8",
    "@types/eslint": "9.6.1",
    "@types/node": "22.10.2",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "autoprefixer": "10.4.20",
    "bits-ui": "0.22.0",
    "clsx": "2.1.1",
    "dotenv": "16.4.7",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.46.1",
    "globals": "15.14.0",
    "histoire": "0.17.17",
    "lucide-svelte": "0.468.0",
    "openapi-typescript": "7.4.4",
    "openapi-typescript-helpers": "^0.0.15",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "prettier-plugin-svelte": "3.3.2",
    "prettier-plugin-tailwindcss": "0.6.9",
    "svelte": "4.2.18",
    "svelte-check": "4.1.1",
    "svelte-legos": "^0.2.5",
    "tailwind-merge": "2.5.5",
    "tailwind-variants": "0.3.0",
    "tailwindcss": "3.4.17",
    "ts-essentials": "^10.0.4",
    "tsafe": "^1.8.5",
    "type-fest": "^4.37.0",
    "typescript": "5.7.2",
    "typescript-eslint": "8.20.0",
    "vite": "5.1.8",
    "vite-plugin-kit-routes": "0.7.0",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "@fontsource-variable/noto-sans": "^5.1.1",
    "@internationalized/date": "3.6.0",
    "@lukemorales/query-key-factory": "1.3.4",
    "@prgm/sveltekit-progress-bar": "2.0.0",
    "@tanstack/svelte-query": "5.62.8",
    "bigger-picture": "1.1.19",
    "cmdk-sv": "0.0.18",
    "date-fns": "^4.1.0",
    "es-toolkit": "^1.32.0",
    "formsnap": "1.0.1",
    "openapi-fetch": "0.13.3",
    "svelte-headless-table": "0.18.3",
    "svelte-persisted-store": "0.12.0",
    "svelte-sonner": "0.3.28",
    "sveltekit-superforms": "2.22.1",
    "uqr": "^0.1.2",
    "zod": "3.24.1"
  }
}
